@model IEnumerable<CareeriaIOTSensorControl.Models.Commands>

@{
    ViewBag.Title = "Index";
}

<h2>Commands</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table tble-striped">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.DeviceId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Command)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Executed)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.DeviceId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Command)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Executed)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id_Command }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id_Command }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id_Command })
        </td>
    </tr>
}

</table>
